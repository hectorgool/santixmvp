<div class="col-md-6 col-md-offset-3">
	<div class="panel panel-default">
  		<div class="panel-body">
    		<div class="page-header">
      			<h1>User</h1>
      		</div>
      		<dl class="dl-horizontal">
				<dt>
					Id
				</dt>
				<dd>
					<%= @user.id %>		
				</dd>

				<dt>
					Firstname
				</dt>
				<dd>
					<%= @user.firstname %>		
				</dd>

				<dt>
					Lastname
				</dt>
				<dd>
					<%= @user.lastname %>		
				</dd>

				<dt>
					Email
				</dt>
				<dd>
					<%= @user.email %>		
				</dd>

				<dt>
					Created at
				</dt>
				<dd>
					<%= @user.created_at %>		
				</dd>

				<dt>
					Updated at
				</dt>
				<dd>
					<%= @user.updated_at %>		
				</dd>

			</dl>
      	</div>
    </div>
</div>